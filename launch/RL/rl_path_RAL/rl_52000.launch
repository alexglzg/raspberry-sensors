<launch>

    <node pkg="sensors" type="imu" name="imu" />

    <node pkg="rosserial_python" type="serial_node.py" name="serial_node" args = "/dev/ttyUSB1" />

    <node pkg="sensors" type="ardumotors.py" name="ardumotors" />

    <node name="rl_control" pkg="sensors" type="rl_control.py" >
	<param name = "model" value = "example52000" />
    </node>

    <!--Launch rosbag to record topics -->
    <node type="record" pkg="rosbag" name="recorder" args="record -a -o /home/ubuntu/catkin_ws/src/sensors/bags/rl/52000" />

</launch>
